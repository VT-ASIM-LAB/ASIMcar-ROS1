<launch>
  <node ns="from_cam" name="gscam" pkg="gscam" type="gscam" output="screen">
    <param name="camera_name" value="default"/>
    <param name="camera_info_url" value="package://gscam/examples/uncalibrated_parameters.ini"/>
    <param name="image_encoding" value="mono8"/>
    <param name="gscam_config" value="v4l2src device=/dev/video1 ! image/jpeg,width=1920,height=1080,framerate=30/1 ! jpegdec ! videoconvert"/>
    <param name="use_gst_timestamps" value="false"/>
    <param name="sync_sink" value="false"/>
  </node>

</launch>
